					}
				},
            },{
                categories: [1999,2000,2001,2002,2003,2004,2005,2006,2007,2008,2009],
				labels: {
					style: {
						color:  '#ba0f27'
					}
				},
                opposite: true
            }],
			

			
			//Y Axes!!!
            yAxis: [{ // Primary yAxis
            title: {
                text: 'Hanging suicides',
                style: {
                    color: '#000000'
                }
            },
            labels: {
                format: '{value} suicides',
                style: {
                    color: '#000000'
                }
            },
			max:9000,            opposite: true
        },{ // 2ndary yAxis
            labels: {
                format: '${value} billion',
                style: {
                    color: '#ba0f27'
                }
            },
			max:30,            title: {
                text: 'US spending on science',
                style: {
                    color: '#ba0f27'
                }
            }
        }],
		        
				
				
		plotOptions: {
            series: {
                states: {
                    hover: {
                        enabled: true,
                        lineWidth: 2
                    }
                }
            }
        },
        exporting: {
            sourceWidth: 1040,
            sourceHeight: 410,
            scale: 2,
            chartOptions: {
                subtitle: null
            }
        },
				
            tooltip: {
                shared: true
            },
            series: [{
                name: 'Hanging suicides',
                color: '#000000',
                type: 'spline',
                data: [5427,5688,6198,6462,6635,7336,7248,7491,8161,8578,9000],
